{
  "transform2D": {
    "position": { "x": 0, "y": 0, "z": 10 },
    "scale": { "x": 1, "y": 1, "z": 1 },
    "anchor": { "x": 0.5, "y": 0.5, "z": 0.5 },
    "rotation": { "x": 0, "y": 0, "z": 0 }
  },
    "camera2D": {
        "fov": 60,
        "aspect ": 1.777777777777778,
        "near": 0.1,
        "far": 1000,
        "isActive": true
    },
    "tween2D": {
        "tweens": {
            "fadeIn": {
                "props": {
                "alpha": 1
                },
                "duration": 0.5,
                "ease": "linear"
            },
            "flyUp": {
                "props": {
                "y": "-2",
                "alpha": 0.5
                },
                "duration": 1,
                "ease": "power1.inOut"
            },
            "scroll": {
                "props": {
                "x": "5"
              },
                "duration": 5,
                "ease": "power1.inOut"
            }
        }
    }
}